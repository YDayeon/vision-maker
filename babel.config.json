{
  "presets": ["next/babel", "@babel/typescript"],
  "plugins": [
    "@babel/proposal-object-rest-spread",
    "babel-plugin-styled-components",
    "babel-plugin-macros",
    [
      "module-resolver",
      {
        "root": ["."],
        "alias": {
          "@": "./src"
        },
        "extensions": [".js", ".jsx", ".tsx"]
      }
    ]
  ]
}
